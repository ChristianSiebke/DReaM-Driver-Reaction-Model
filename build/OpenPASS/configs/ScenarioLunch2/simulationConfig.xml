  </Observations>
  <Spawners>
	<Spawner>
      <Library>SpawnerRuntimeCommon</Library>
      <Type>Runtime</Type>
      <Priority>0</Priority>
      <Profile>SpawnPoint Road:0 Car</Profile>
    </Spawner>		
	<Spawner>
      <Library>SpawnerRuntimeCommon</Library>
      <Type>Runtime</Type>
      <Priority>0</Priority>
      <Profile>SpawnPoint Road:0 Cyclist</Profile>
    </Spawner>	
	<Spawner>
      <Library>SpawnerRuntimeCommon</Library>
